            <Text layoutX="93.0" layoutY="255.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter an expression:" />
            <Text fx:id="time" layoutX="436.0" layoutY="257.0" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00">
               <font>
                  <Font size="19.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
