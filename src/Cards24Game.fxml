<?import javafx.scene.text.Text?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Cards24GameController">
   <children>
      <Pane fx:id="rootPane" prefHeight="284.0" prefWidth="519.0">
         <children>
            <Pane fx:id="cardPane" layoutX="14.0" layoutY="42.0" prefHeight="190.0" prefWidth="482.0" />
            <Button fx:id="findASolutionBtn" layoutX="71.0" layoutY="14.0" mnemonicParsing="false" prefWidth="113.0" text="Loading Solutions" />
